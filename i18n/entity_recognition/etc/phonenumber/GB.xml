<phone_number_rule>
    <negative>
        <rule>
            <insensitive>True</insensitive>
            <type>ZERO</type>
            <valid>Default</valid>
            <handle>Default</handle>
            <content>(?&lt;!\p{L})(Jan|Feb|Mar|Apr|May|Jun|Jul|Aug|Sep|Oct|Nov|Dec|January|February|March|April|May|June|</content>
            <content>July|August|September|October|November|December)\p{Blank}*(-|\p{Blank})?\p{Blank}*([012]?\d|3[01])</content>
            <content>\p{Blank}*(-|\p{Blank})?\p{Blank}*(1[4-9]\d{2}|20[01][0-9])(\p{Blank}*)(([01]?\d|2[0-4])\p{Blank}</content>
            <content>*[.:]\p{Blank}*[0-5]\d(\p{Blank}*[.:]\p{Blank}*[0-5]\d)?)?</content>
        </rule>
        <rule>
            <insensitive>True</insensitive>
            <type>ZERO</type>
            <valid>Default</valid>
            <handle>Default</handle>
            <content>\d[0-9\p{Blank}.,-]*\d\p{Blank}{0,4}(kilobytes|kilowatt-hours|megabytes|kilojoules|square\p{Blank}+</content>
            <content>centimetres|gal\p{Blank}+US|kilohertz|byte|calories|milliamperes|MB|m²|MW|hours|gigahertz|kilograms|pints|</content>
            <content>oz|ha|bit|Tb|pm|years|Mb|bytes|A|hPa|pounds|GB|carats|degrees\p{Blank}+Celsius|J|K|picometres|W|V|secs|</content>
            <content>deg|mg|dm|dl|decimetres|hectopascals|wks|megawatts|ml|centimetres|kHz|joules|ounces|g|decilitres|mm|Gb|</content>
            <content>ms|l|mins|m|nmi|millimetres|weeks|yards|kWh|days|seconds|gram|volts|tn|nautical\p{Blank}+miles|kilocalories|</content>
            <content>kW|Calories|yrs|US\p{Blank}+gallons|cm²|degrees\p{Blank}+Fahrenheit|kg|Ω|amperes|kilometres|kb|milliseconds|</content>
            <content>tons|kilowatts|gigabytes|hertz|megahertz|mths|watts|kilobits|litres|km|Hz|cm|GHz|gigabits|mA|°|lb|</content>
            <content>millilitres|Cal|metres|ohms|hectares|milligrams|hrs|kelvin|bits|cal|MHz|TB|megabits|°C|square\p{Blank}+metres|</content>
            <content>minutes|°Flyd|terabits|kcal|degrees|months|kJ|CD|pt|terabytes|kB)(?!\p{L})</content>
        </rule>
        <rule>
            <insensitive>False</insensitive>
            <type>ZERO</type>
            <valid>Default</valid>
            <handle>Default</handle>
            <content>\p{Sc}\d+[.]?\d+</content>
        </rule>
        <rule>
            <insensitive>False</insensitive>
            <type>ZERO</type>
            <valid>Default</valid>
            <handle>Default</handle>
            <content>4x\d0+</content>
        </rule>
        <rule>
            <insensitive>False</insensitive>
            <type>ZERO</type>
            <valid>Default</valid>
            <handle>Default</handle>
            <content>&amp;amp;#\d{2,5}</content>
        </rule>
        <rule>
            <insensitive>False</insensitive>
            <type>ZERO</type>
            <valid>Default</valid>
            <handle>Default</handle>
            <content>\d+((th)|%)</content>
        </rule>
        <rule>
            <insensitive>False</insensitive>
            <type>ZERO</type>
            <valid>Default</valid>
            <handle>Default</handle>
            <content>(?&lt;!(call))[iI]n \d{4}</content>
        </rule>
        <rule>
            <insensitive>False</insensitive>
            <type>ZERO</type>
            <valid>Default</valid>
            <handle>Default</handle>
            <content>([fF]rom|[uU]ntil) \d{4}</content>
        </rule>
        <rule>
            <insensitive>False</insensitive>
            <type>ZERO</type>
            <valid>Default</valid>
            <handle>Default</handle>
            <content>\d{4,}[+]</content>
        </rule>
        <rule>
            <insensitive>False</insensitive>
            <type>ZERO</type>
            <valid>Default</valid>
            <handle>Default</handle>
            <content>20[01]\d\p{Blank}{0,2}-\p{Blank}{0,2}20[01]\d</content>
        </rule>
        <rule>
            <insensitive>False</insensitive>
            <type>ZERO</type>
            <valid>Default</valid>
            <handle>Default</handle>
            <content>(?&lt;!\d)[1-9][0-9]{0,2}(?:(?:[,\p{Blank}]\d{3})*|\d{0,13})(\.\d{1,3})(?!\d)</content>
        </rule>
    </negative>
    <positive>
        <rule>
            <insensitive>False</insensitive>
            <type>ZERO</type>
            <valid>Default</valid>
            <handle>StartWithMobile</handle>
            <content>([mM]ob(ile)?|[cC]all|landline|fixedline)[:]?\s*+\d{5,}+</content>
        </rule>
        <rule>
            <insensitive>False</insensitive>
            <type>ZERO</type>
            <valid>Default</valid>
            <handle>EndWithMobile</handle>
            <content>\d{5,}+\s*+[\(]?mob</content>
        </rule>
    </positive>
    <border>
        <rule>
            <insensitive>True</insensitive>
            <type>CONTAIN</type>
            <valid>Default</valid>
            <handle>Default</handle>
            <content>(0{3,}|1{3,}|2{3,}|3{3,}|4{3,}|5{3,}|6{3,}|7{3,}|8{3,}|9{3,})</content>
        </rule>
        <rule>
            <insensitive>True</insensitive>
            <type>CONTAIN</type>
            <valid>Default</valid>
            <handle>Default</handle>
            <content>[\(\[]?1\d(0\d|1[012])([012]\d|3[01])</content>
        </rule>
        <rule>
            <insensitive>True</insensitive>
            <type>CONTAIN</type>
            <valid>Default</valid>
            <handle>Default</handle>
            <content>[1-9]0{3,10}</content>
        </rule>
    </border>
    <codes>
        <rule>
            <insensitive>False</insensitive>
            <type>ZERO</type>
            <valid>Rawstr</valid>
            <handle>Default</handle>
            <content></content>
        </rule>
        <rule>
            <insensitive>False</insensitive>
            <type>ZERO</type>
            <valid>PreSuf</valid>
            <handle>Default</handle>
            <content></content>
        </rule>
    </codes>
    <common_exit>
        <rule>
            <content>True</content>
        </rule>
    </common_exit>
</phone_number_rule>